{
  "name": "api",
  "version": "1.0.0",
  "description": "crawler api",
  "main": "app/dist/main.js",
  "scripts": {
    "db:seed": "tsx prisma/seeders/run.ts",
    "db:reset": "prisma migrate reset --force",
    "clean": "tsc --build --clean",
    "start": "npm run clean && tsc && tsc-alias && node ./dist/src/main.js",
    "dev": "tsx src/main.ts",
    "watch": "tsx watch src/main.ts",
    "format": "prettier --write ./src/**/*",
    "db.test:reset": "DATABASE_URL=postgresql://postgres:password@db_test:5432/crawler?schema=public prisma migrate reset --force",
    "test": "npm run db.test:reset && jest --no-cache --runInBand"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.13",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0"
  },
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "@types/cors": "^2.8.17",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "luxon": "^3.4.4",
    "prisma": "^5.9.0"
  }
}
